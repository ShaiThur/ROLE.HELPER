              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "The service has successfully processed the request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/SystemResponse"
                }
              }
            }
          },
          "422": {
            "description": "Validation Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/HTTPValidationError"
                }
              }
            }
          }
        }
      }
    },
    "/llm/ask": {
      "post": {
        "tags": [
          "main controller"
        ],
        "summary": "Ask Llm",
        "operationId": "ask_llm_llm_ask_post",
